{
"name":"Email GitHub Issues",
"description":"None",
"version":"1",
"manifest_version":2,
"options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
"web_accessible_resources": [
  "scripts/injectedcode.js",
  "images/loading.gif"
  ],
"content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["jquery-2.1.4.min.js", "addemailbuttons.js"],
      "run_at": "document_start"
    }
  ],
"background": {
  "scripts": ["background.js"],
  "persistent": false
  },
"browser_action": {
    "default_icon": "icon.png"
  },
"permissions": [
  "tabs",
  "storage",
  "https://ajax.googleapis.com/",
  "https://github.com/*",
  "http://dotnet-ci.cloudapp.net/*"
  ]
}