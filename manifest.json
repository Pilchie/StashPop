{
"name":"Email GitHub Issues",
"description":"None",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["addemailbuttons.js"],
      "run_at": "document_start"
    }
  ],
"background": {
  "scripts": ["background.js"],
  "persistent": false
  },
"browser_action": {
    "default_icon": "icon.png"
  },
"permissions": [
  "tabs",
  "https://ajax.googleapis.com/",
  "https://github.com/*",
  "http://dotnet-ci.cloudapp.net/*"
  ]
}